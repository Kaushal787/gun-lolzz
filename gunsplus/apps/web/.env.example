# Admin login (at /admin/login)
ADMIN_PASSWORD=admin

# Optional admin API bypass header for write routes (x-admin)
ADMIN_SECRET=

# Where to store JSON profiles (defaults to apps/web/data)
DATA_DIR=

# Optional: external API base if you want to proxy /api/backend/*
API_URL=

# Optional: to fetch Discord avatar/tag by userId via /api/discord/avatar
DISCORD_BOT_TOKEN=

# Default public profile to show (only this page is public)
DEFAULT_PUBLIC_PROFILE=ZEUS
